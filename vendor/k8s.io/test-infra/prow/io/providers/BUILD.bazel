load("@io_bazel_rules_go//go:def.bzl", "go_library")

go_library(
    name = "go_default_library",
    srcs = ["providers.go"],
    importmap = "kubevirt.io/project-infra/vendor/k8s.io/test-infra/prow/io/providers",
    importpath = "k8s.io/test-infra/prow/io/providers",
    visibility = ["//visibility:public"],
    deps = [
        "//vendor/github.com/aws/aws-sdk-go/aws:go_default_library",
        "//vendor/github.com/aws/aws-sdk-go/aws/credentials:go_default_library",
        "//vendor/github.com/aws/aws-sdk-go/aws/session:go_default_library",
        "//vendor/gocloud.dev/blob:go_default_library",
        "//vendor/gocloud.dev/blob/memblob:go_default_library",
        "//vendor/gocloud.dev/blob/s3blob:go_default_library",
    ],
)

filegroup(
    name = "package-srcs",
    srcs = glob(["**"]),
    tags = ["automanaged"],
    visibility = ["//visibility:private"],
)

filegroup(
    name = "all-srcs",
    srcs = [":package-srcs"],
    tags = ["automanaged"],
    visibility = ["//visibility:public"],
)
